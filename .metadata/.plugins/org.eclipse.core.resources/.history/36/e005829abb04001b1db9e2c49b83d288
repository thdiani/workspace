package br.com.lojauati.teste;

import br.com.lojauati.modelo.Elevador;

public class TesteElevador {

	public static void main(String[] args) {
		//preencher estatico os paramentros no momento de estanciar o objeto
		Elevador objeto = new Elevador(25,100);
		System.out.println(objeto.exibirTudo());
		objeto.preencherCapacidadePessoas(10);
		objeto.preencherMaiorAndar(20);
		objeto.preenhcerMenorAndar(0);
		System.out.println("===============================");
		System.out.println(objeto.exibirTudo());
		objeto.entrar(5);
		objeto.subir();
		objeto.subir();
		objeto.subir();
		objeto.sair(2);
		System.out.println("===============================");
		System.out.println(objeto.exibirTudo());
	}

}
